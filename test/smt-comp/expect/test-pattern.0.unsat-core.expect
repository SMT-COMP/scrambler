(set-option :print-success false)
(set-option :produce-unsat-cores true)
(set-logic UF)
(declare-sort A 0)
(declare-fun a () A)
(declare-fun f (A) Bool)
(assert (! (not (f a)) :named smtcomp1))
(assert (! (forall ((?x A)) (! (f ?x) :pattern ((f ?x)))) :named smtcomp2))
(check-sat)
(get-unsat-core)
(exit)
