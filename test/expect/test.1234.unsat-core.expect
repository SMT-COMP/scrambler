(set-option :produce-unsat-cores true)
(set-logic ALL)
(declare-fun x3 () Bool)
(declare-fun x2 () Bool)
(declare-fun x1 () Bool)
(declare-fun x5 () Bool)
(push 1)
(assert (! (let ((x4 (not x2))) x4) :named smtcomp1))
(check-sat)
(get-unsat-core)
(pop 1)
(assert (! (xor x2 x3) :named smtcomp2))
(push 1)
(assert (! (and x1 (not x3) x5) :named smtcomp3))
(check-sat)
(get-unsat-core)
(pop 1)
(check-sat)
(get-unsat-core)
(exit)
